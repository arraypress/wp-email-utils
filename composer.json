{
  "name": "arraypress/wp-email-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for email address operations, validation, anonymization, and privacy-compliant handling with comprehensive disposable domain detection",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-email-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\EmailUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-email-utils/issues",
    "source": "https://github.com/arraypress/wp-email-utils"
  },
  "keywords": [
    "wordpress",
    "email",
    "validation",
    "anonymization",
    "privacy",
    "gdpr",
    "email-utils",
    "bulk-operations",
    "subaddressing",
    "email-filtering",
    "spam-detection",
    "disposable-email",
    "email-extraction",
    "punycode",
    "arraypress"
  ]
}